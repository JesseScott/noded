<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.ecuad.noded" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>noded</name>
    <description>
        a notepad for nodes.
    </description>
    <author email="jcscott@ecuad.ca" href="http://www.ecuad.ca/people/profile/234660">
        Jesse Scott | Emily Carr University
    </author>
    <content src="index.html" />
    <access origin="*" />

    <feature name="Geolocation">
      <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="Geolocation">
      <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>

</widget>
